{
  "name": "karma-read-json5",
  "version": "0.0.3",
  "description": "Karma plugin to load JSON (and use if possible JSON5)",
  "main": "lib/index.js",
  "scripts": {
    "bumping": "node ./node_modules/npmversion/bin/npmversion",
    "bump-release": "npm test && npm run bumping -- --unpreid",
    "bump-major": "npm test && npm run bumping -- --increment major",
    "bump-minor": "npm test && npm run bumping -- --increment minor",
    "bump-patch": "npm test && npm run bumping -- --increment patch",
    "bump-major-beta": "npm run bumping -- --increment major --preid beta",
    "bump-minor-beta": "npm run bumping -- --increment minor --preid beta",
    "bump-patch-beta": "npm run bumping -- --increment patch --preid beta"
  },
  "homepage": "https://github.com/rochejul/karma-read-json5",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rochejul/karma-read-json5.git"
  },
  "bugs": {
    "url": "https://github.com/rochejul/karma-read-json5/issues"
  },
  "keywords": [
    "karma-plugin",
    "karma-adapter",
    "json",
    "json5"
  ],
  "author": {
    "name": "Julien Roche"
  },
  "contributors": [],
  "license": "MIT",
  "engines": {
    "node": ">=4.0.0",
    "npm": ">=2.0.0"
  },
  "dependencies": {
    "json5": "latest"
  },
  "devDependencies": {
    "npmversion": "1.3.4"
  }
}
